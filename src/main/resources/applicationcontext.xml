<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id = "user" class="com.springioc.innerbean.beans.User">
<property name="name" value="Amandeep Singh"></property>
<property name="car">
<bean id = "car" class="com.springioc.innerbean.beans.Car">
<property name="name" value="Renault Duster"></property>
<property name="price" value="40000000"></property>
</bean>
</property>
</bean>
</beans>
